keywords = {Austempered ductile iron,Discrete laser spot hardening,Laser process parameter map,Laser surface melting},
month = {dec},
pages = {143--152},
publisher = {Elsevier},
title = {{Discrete laser spot hardening of austempered ductile iron}},
url = {https://www.sciencedirect.com/science/article/pii/S0257897217310952},
volume = {331},
year = {2017}
}
