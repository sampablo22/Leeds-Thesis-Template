keywords = {TA Engineering (General). Civil engineering (Gener},
month = {aug},
number = {4},
pages = {179--191},
publisher = {Elsevier},
title = {{The sand erosion performance of coatings}},
url = {https://www.sciencedirect.com/science/article/pii/S0261306999000242{\#}FIG1 http://dx.doi.org/10.1016/S0261-3069(99)00024-2},
volume = {20},
