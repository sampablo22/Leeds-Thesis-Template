isbn = {0301-679X},
issn = {0301679X},
journal = {Tribol. Int.},
keywords = {Composites,Friction,Multilayer coating,Sliding wear,diamond-like carbon},
month = {nov},
number = {7},
pages = {559--570},
publisher = {Elsevier},
title = {{Multilayer composite ceramic-metal-DLC coatings for sliding wear applications}},
url = {https://www.sciencedirect.com/science/article/pii/0301679X9500121J},
volume = {29},
