@article{Topolovec-Miklozic2008,
abstract = {The friction properties of a range of boundary lubricating additives in diamond-like carbon (DLC)-DLC rolling-sliding contact have been investigated. Two types of commercial DLC have been studied, one hydrogenated diamond-like and the other Cr-doped, non-hydrogenated and graphitic. The graphitic-type DLC coating initially gave very low boundary friction with additive-free base oil but after a few minutes of rubbing in thin film conditions friction rose to a higher value, similar to that produced immediately by the diamond-like carbon. Some organic friction modifiers were able to preserve this low boundary friction behaviour of the graphitic-type DLC for a full 2-h rubbing test. One organic friction modifier, glycerol monooleate produced a considerable reduction in friction with both types of DLC coating in intermediate speed, though noting very slow speed conditions. The additive molybdenum dithiocarbamate formed a low friction film on both DLC surfaces, with lower friction on the graphitic than on diamond-like carbon one. Atomic force microscopy (AFM) measurements showed that this was due to the formation of regions of low lateral force on asperities within the rubbed track. ZDDP provided some reduction in friction on both DLC surfaces after rubbing but did not form thick ZDDP tribofilms like those generated on steel surfaces. ?? 2008 Elsevier B.V. All rights reserved.},
author = {Topolovec-Miklozic, Ksenija and Lockwood, Frances and Spikes, Hugh},
doi = {10.1016/j.wear.2008.04.051},
file = {:C$\backslash$:/Users/sammc/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Topolovec-Miklozic, Lockwood, Spikes - 2008 - Behaviour of boundary lubricating additives on DLC coatings.pdf:pdf},
isbn = {0043-1648},
issn = {00431648},
journal = {Wear},
keywords = {AFM,DLC,Friction,Friction modifier additives},
number = {11-12},
pages = {1893--1901},
title = {{Behaviour of boundary lubricating additives on DLC coatings}},
volume = {265},
year = {2008}
}
@incollection{Beake2017,
abstract = {Depending on their composition diamond-like carbon (DLC) films can have a very wide range of mechanical properties. Accurate nanomechanical characterisation is a critical step in their efficient optimisation for demanding applications. This chapter highlights the key factors that are required for accurate hardness and elastic modulus determination and the further complications that arise when measuring on ultra-thin DLC films. It tabulates a comparison of mechanical properties and an indication of industrial use of DLC coatings deposited using plasma-enhanced chemical vapor deposition (PECVD) and physical vapor deposition (PVD) methods. To optimise the performance of DLC coatings operating in demanding contact loading conditions it is useful to assess their mechanical behaviour under more relevant loading conditions than solely in nanoindentation. The chapter describes a multi-technique approach to optimize DLC behavior. The approach combines overload tests with fatigue and durability experiments and it includes a suite of nano-mechanical testing methods, namely nano-indentation, nano-scratch, nano-impact and nano-fretting.},
author = {Beake, Ben D. and Liskiewicz, Tomasz},
