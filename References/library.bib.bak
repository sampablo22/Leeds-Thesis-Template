journal = {Diam. Relat. Mater.},
month = {apr},
number = {4-6},
pages = {361--368},
publisher = {Elsevier},
title = {{The deposition mechanism of diamond-like a-C and a-C: H}},
url = {http://www.sciencedirect.com/science/article/pii/0925963594901864},
volume = {3},
year = {1994}
}
