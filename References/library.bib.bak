issn = {13596454},
journal = {Acta Mater.},
keywords = {Magnetron sputtering,Nanostructure,Properties,Thin films,TiC/a-C:H nanocomposite},
month = {oct},
number = {17},
pages = {4505--4521},
title = {{Nanostructure and properties of TiC/a-C:H composite coatings}},
