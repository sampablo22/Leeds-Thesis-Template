isbn = {0257-8972},
issn = {02578972},
journal = {Surf. Coatings Technol.},
keywords = {Composite hardness,Critical load,Indenter radius,Scratch depth,Scratch hardness},
month = {feb},
number = {1},
pages = {1--7},
publisher = {Elsevier},
title = {{Effects of indenter radius on the critical load in scratch testing}},
url = {https://www.sciencedirect.com/science/article/pii/S0257897202007181},
volume = {165},
year = {2003}
}
