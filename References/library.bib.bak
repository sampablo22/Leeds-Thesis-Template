keywords = {Diamond-like carbon,Fretting fatigue,Fretting wear,Graphite-like carbon,Titanium alloy},
month = {sep},
pages = {462--469},
publisher = {North-Holland},
title = {{Fretting wear and fretting fatigue behaviors of diamond-like carbon and graphite-like carbon films deposited on Ti-6Al-4V alloy}},
