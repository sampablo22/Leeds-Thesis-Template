publisher = {Elsevier},
title = {{Wear and corrosion properties of diamond like carbon (DLC) coating on stainless steel, CoCrMo and Ti6Al4V substrates}},
url = {https://www.sciencedirect.com/science/article/pii/S0257897215002443},
volume = {273},
year = {2015}
}
