file = {:C$\backslash$:/Users/sammc/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fouvry et al. - 1996 - Theoretical analysis of fatigue cracking under dry friction for fretting loading conditions.pdf:pdf},
issn = {00431648},
