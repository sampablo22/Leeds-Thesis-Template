}
@inproceedings{Kubiak2011,
abstract = {Influence of initial surface roughness on friction and wear processes under fretting conditions was investigated experimentally. Rough surfaces (Ra=0.152.52 $\mu$m) were prepared on two materials: carbon alloy (AISI 1034) and titanium alloy (Ti-6Al-4V). Strong influence of initial surface roughness on friction and wear processes is reported for both tested materials. Lower coefficient of friction and increase in wear rate was observed for rough surfaces. Wear activation energy is increasing for smoother surfaces. Lower initial roughness of surface subjected to gross slip fretting can delay activation of wear process and reduce wear rate; however, it can slightly increase the coefficient of friction. {\textcopyright} 2011 Elsevier Ltd.},
