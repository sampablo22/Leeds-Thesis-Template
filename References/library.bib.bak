pages = {999--1002},
publisher = {Elsevier},
title = {{Electrical and optical properties of diamond-like carbon films deposited by pulsed laser ablation}},
url = {https://www.sciencedirect.com/science/article/pii/S0925963510001330},
volume = {19},
