publisher = {Elsevier},
title = {{Introduction of a fretting-fatigue mapping concept: Development of a dual crack nucleation - crack propagation approach to formalize fretting-fatigue damage}},
url = {https://www.sciencedirect.com/science/article/pii/S0142112308002168?via{\%}3Dihub{\#}fig2},
volume = {31},
year = {2009}
}
