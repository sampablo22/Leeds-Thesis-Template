doi = {10.1016/0022-5088(68)90115-X},
file = {:C$\backslash$:/Users/sammc/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Byron - 1968 - Plastic deformation of tantalum single crystals. II. The orientation dependence of yield.pdf:pdf},
