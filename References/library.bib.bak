journal = {J. Alloys Compd.},
keywords = {Cyclic impact-sliding test,DLC coating,Failure mechanisms,Metallic biomaterials},
month = {dec},
pages = {964--973},
publisher = {Elsevier},
title = {{Study on failure mechanisms of DLC coated Ti6Al4V and CoCr under cyclic high combined contact stress}},
url = {https://www.sciencedirect.com/science/article/pii/S0925838816322897},
volume = {688},
