journal = {Surf. Coatings Technol.},
keywords = {Adhesion,Carbon nitride,Coating,HiPIMS,Pretreatment,Steel substrate},
month = {sep},
pages = {454--462},
publisher = {Elsevier},
title = {{Improved adhesion of carbon nitride coatings on steel substrates using metal HiPIMS pretreatments}},
url = {https://www.sciencedirect.com/science/article/pii/S0257897216305400},
volume = {302},
year = {2016}
