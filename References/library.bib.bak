keywords = {Dynamic indentation,Erosion,Impact,Tribologically transformed surface},
number = {3},
pages = {263--274},
publisher = {Kluwer Academic Publishers-Plenum Publishers},
title = {{A micro/macro impact test at controlled energy for erosion and phase-transformation simulation}},
url = {http://link.springer.com/10.1023/A:1024996621189 http://www.scopus.com/inward/record.url?eid=2-s2.0-0242415984{\&}partnerID=tZOtx3y1},
