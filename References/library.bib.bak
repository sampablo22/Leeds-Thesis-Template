journal = {Wear},
month = {oct},
pages = {203028},
publisher = {Elsevier},
title = {{Tribological behavior of microwave synthesized high entropy alloy claddings}},
url = {https://www.sciencedirect.com/science/article/pii/S0043164819302145 https://linkinghub.elsevier.com/retrieve/pii/S0043164819302145},
