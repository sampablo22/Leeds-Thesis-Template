keywords = {Raman spectra,dangling bonds,defect states,density,diamond-like carbon,hydrogen,molecular dynamics method,photoluminescence,plasma CVD,rapid thermal annealing,sputter deposition,thin films,vacuum deposition},
month = {sep},
