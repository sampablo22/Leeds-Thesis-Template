journal = {Surf. Coatings Technol.},
keywords = {Applications of ta-C,DLC history,Doped a-C,Filtered arc,Pulsed arc,ta-C},
month = {oct},
pages = {213--240},
publisher = {Elsevier},
title = {{60years of DLC coatings: Historical highlights and technical review of cathodic arc processes to synthesize various DLC types, and their evolution for industrial applications}},
