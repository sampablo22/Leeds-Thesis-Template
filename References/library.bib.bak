journal = {J. Biomed. Mater. Res. Part B Appl. Biomater.},
keywords = {DLC,biomaterials,carbon,hemocompatibility,joint replacement,stent},
month = {oct},
number = {1},
pages = {72--84},
pmid = {17285609},
publisher = {Wiley-Blackwell},
title = {{Biomedical applications of diamond-like carbon coatings: A review}},
