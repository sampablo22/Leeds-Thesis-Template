isbn = {1057-7157},
issn = {10577157},
journal = {J. Microelectromechanical Syst.},
keywords = {Atomic force microscopy (AFM),Boundary lubrication,Ionic liquid (IL),Microelectromechanical system (MEMS)},
number = {4},
