isbn = {0257-8972},
issn = {02578972},
journal = {Surf. Coatings Technol.},
keywords = {Magnetron sputtering,Mechanical properties,Microindentation measurements,Nanocomposite films},
month = {may},
number = {2-3},
pages = {304--313},
title = {{Relationships between hardness, Young's modulus and elastic recovery in hard nanocomposite coatings}},
