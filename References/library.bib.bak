issn = {02578972},
journal = {Surf. Coatings Technol.},
month = {jun},
number = {22-23 SPEC. ISS.},
pages = {6405--6408},
publisher = {Elsevier},
title = {{Control of stress and delamination in single and multi-layer carbon thin films prepared by cathodic arc and RF plasma deposition and implantation}},
url = {https://www.sciencedirect.com/science/article/pii/S025789720501203X},
volume = {200},
