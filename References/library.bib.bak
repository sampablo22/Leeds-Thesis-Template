file = {:C$\backslash$:/Users/sammc/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hacisalihoglu et al. - 2014 - Tribocorrosion properties of different type titanium alloys in simulated body fluid(2).pdf:pdf},
issn = {00431648},
journal = {Wear},
keywords = {?? type Ti alloys,Biomaterials,SBF,Titanium and its alloys,Tribocorrosion},
month = {may},
pages = {679--686},
title = {{Tribocorrosion properties of different type titanium alloys in simulated body fluid}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0043164814003950},
volume = {332-333},
year = {2014}
